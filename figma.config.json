{
  "codeConnect": {
    "include": [
      "src/ui/**/*",
      "src/figma/**/*"
    ],
    "exclude": [
      "src/figma/icons/*"
    ],
    "parser": "react",
    "importPaths": {
      "ui/primitives/BasicButtonContained": "primitives"
    },
    "paths": {
      "compositions": [
        "src/ui/compositions"
      ],
      "hooks": [
        "src/ui/hooks"
      ],
      "icons": [
        "src/ui/icons"
      ],
      "images": [
        "src/ui/images"
      ],
      "layout": [
        "src/ui/layout"
      ],
      "primitives": [
        "src/ui/primitives"
      ],
      "providers": [
        "src/ui/providers"
      ],
      "utils": [
        "src/ui/utils"
      ]
    },
    "documentUrlSubstitutions": {
      "<FIGMA_ICONS_BASE>": "https://www.figma.com/design/2TnwsFNeqz0UQXhdDlFeuy"
    }
  }
}